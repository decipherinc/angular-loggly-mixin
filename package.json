{
  "name": "angular-loggly-mixin",
  "version": "0.0.0",
  "description": "AngularJS module to pass $log calls thru to Loggly",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint && npm run karma",
    "karma": "karma start .karma.conf.js --single-run",
    "watch": "karma start .karma.conf.js",
    "lint": "eslint src/**/*.js .*..js test/**/*.js",
    "build": "npm run build-node; npm run build-browser",
    "build-node": "rimraf lib; babel src --out-dir lib",
    "build-browser": "npm run build-browser-main; npm run build-browser-min",
    "build-browser-main": "browserify --transform [ exposify --expose [ --angular angular ] ] --plugin [ bannerify --file .banner.txt --pkg package.json ] src --outfile angular-loggly-mixin.js",
    "build-browser-min": "browserify --transform [ exposify --expose [ --angular angular ] ] --plugin [ bannerify --file .banner.txt --pkg package.json ] --global-transform uglifyify src --outfile angular-loggly-mixin.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decipherinc/angular-loggly-mixin.git"
  },
  "keywords": [
    "angular",
    "angularjs",
    "loggly",
    "log",
    "decorator",
    "service",
    "mixin"
  ],
  "files": [
    "lib/**/*.js"
  ],
  "author": "Christopher Hiller <chiller@decipherinc.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/decipherinc/angular-loggly-mixin/issues"
  },
  "homepage": "https://github.com/decipherinc/angular-loggly-mixin#readme",
  "peerDependencies": {
    "angular": "*"
  },
  "devDependencies": {
    "angular": "^1.4.8",
    "angular-mocks": "^1.4.8",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "bannerify": "vekat/bannerify#feature-option",
    "browserify": "^12.0.1",
    "browserify-istanbul": "^0.2.1",
    "browserify-ngannotate": "^2.0.0",
    "chai": "^3.4.1",
    "codeclimate-test-reporter": "^0.1.1",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "eslint-config-semistandard": "^5.0.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "exposify": "^0.5.0",
    "istanbul": "^0.4.1",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-phantomjs-launcher": "^0.2.2",
    "lodash.get": "^3.7.0",
    "lodash.partial": "^3.1.1",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19",
    "pre-commit": "^1.1.2",
    "publish-please": "^1.1.0",
    "rimraf": "^2.5.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "uglifyify": "^3.0.1",
    "wallabify": "0.0.12"
  },
  "dependencies": {
    "format": "^0.2.1"
  },
  "browser": "angular-loggly.js",
  "browserify": {
    "transform": [
      "babelify",
      "browserify-ngannotate"
    ]
  }
}
